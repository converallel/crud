{
    "name": "converallel/crud",
    "description": "Crud plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Xun Chen",
            "role": "Maintainer"
        }
    ],
    "require": {
        "cakephp/cakephp": "3.7.*",
        "cakephp/migrations": "^2.0.0"
    },
    "require-dev": {
        "cakephp/bake": "^1.9.0",
        "phpunit/phpunit": "6.*"
    },
    "autoload": {
        "psr-4": {
            "Crud\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Crud\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}
